swipe:
  3: 
    left: 
      command: 'alt + tab'
    right: 
      command: 'alt + tab'
    up: 
      command: 'xdotool key super'
    down: 
      command: 'xdotool windowminimize <window_id>'
  4:
    left: 
      command: 'xdotool key XF86AudioLowerVolume'
    right: 
      command: 'xdotool key XF86AudioRaiseVolume'
    up: 
      command: 'xdotool set_desktop --relative n+1'
    down: 
        command: 'xdotool set_desktop --relative n-1'
pinch:
  in:
    command: 'xdotool key ctrl+plus'
  out:
     command: 'xdotool key ctrl+minus'

threshold:
  swipe: 0.4
  pinch: 0.4

interval:
  swipe: 0.8
  pinch: 0.1
